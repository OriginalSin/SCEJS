{
	"!name": "SCEJS",
	"SCE": {
		"!type": "fn()",		
		"prototype": {
			"initialize": {
				"!type": "fn(jsonIn: ?)"
			},
			"loadProject": {
				"!type": "fn(prj: +Project)"
			},
			"getLoadedProject": {
				"!type": "fn() -> +Project"
			}
		}
	},
	"Project": {
		"!type": "fn()",
		"prototype": {
			"setActiveStage": {
				"!type": "fn(stage: +Stage)"
			},
			"getActiveStage": {
				"!type": "fn() -> +Stage"
			},
			"addStage": {
				"!type": "fn(stage: +Stage)"
			}
		}
	},
	"Stage": {
		"!type": "fn()",
		"prototype": {
			"setActiveCamera": {
				"!type": "fn(node: +Node)"
			},
			"getActiveCamera": {
				"!type": "fn() -> +Node"
			},
			"setSelectedNode": {
				"!type": "fn(node: +Node)"
			},
			"getSelectedNode": {
				"!type": "fn() -> +Node"
			},
			"addNode": {
				"!type": "fn(node: +Node)"
			},
			"getNodes": {
				"!type": "fn() -> [+Node]"
			},
			"render": {
				"!type": "fn()"
			}
		}
	},
	"Node": {
		"!type": "fn()",
		"prototype": {
			"addComponent": {
				"!type": "fn(component: +Component)"
			},
			"getComponent": {
				"!type": "fn(type: +Constants.COMPONENT_TYPES) -> +ComponentRenderer|+ComponentScreenEffect" 
			},
			"getComponents": {
				"!type": "fn() -> Object"
			},
			"setEnabled": {
				"!type": "fn(enable: bool)"
			}
		}
	},
	"Constants": {
		"EVENT_TYPES": {
			"!type": "fn()",
			"KEY_DOWN": "number",
			"KEY_UP": "number",
			"MOUSE_DOWN": "number",
			"MOUSE_UP": "number",
			"MOUSE_MOVE": "number",
			"MOUSE_WHEEL": "number"
		},
		"COMPONENT_TYPES": {
			"!type": "fn()",
			"KEYBOARD_EVENTS": "number",
			"MOUSE_EVENTS": "number",
			"TRANSFORM": "number",
			"TRANSFORM_TARGET": "number",
			"CONTROLLER_TRANSFORM_TARGET": "number",
			"PROJECTION": "number",
			"RENDERER": "number",
			"SCREEN_EFFECTS": "number"
		}
	},
	"Component": {
		"!type": "fn()",
		"prototype": {
			"getComponents": {
				"!type": "fn()"
			}
		}
	},
	"ComponentRenderer": {
		"!type": "fn()",
		"prototype": {
			"getRComponents": {
				"!type": "fn()"
			}
		}
	},
	"ComponentScreenEffect": {
		"!type": "fn()",
		"prototype": {
			"getSEComponents": {
				"!type": "fn()"
			}
		}
	}
}
